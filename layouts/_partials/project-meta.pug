.meta
  ul.tl
    li= page.language || 'n/a'
    li= page.stargazers_count || 0
    li= page.forks || 0
    // li= page.version || '0.0.0'
    li
      a(href=page.html_url, target='_blank', rel='noopener noreferrer') github
    if page.homepage
      li
        a(href=page.homepage, target='_blank', rel='noopener noreferrer') view
  ul.tl
    li language
    li stars
    li forks
    // li version
    li source
    if page.homepage
      li website
